digraph {
	rankdir=LR size="8,5"
	0 [shape=circle]
	01 [shape=circle]
	012 [shape=doublecircle]
	02 [shape=doublecircle]
	2 [shape=doublecircle]
	23 [shape=doublecircle]
	"" [shape=none]
	"" -> 0
	0 -> 01 [label=a]
	0 -> 0 [label=b]
	0 -> 2 [label=c]
	01 -> 012 [label=a]
	01 -> 02 [label=b]
	01 -> 23 [label=c]
	012 -> 012 [label=a]
	012 -> 02 [label=b]
	012 -> 23 [label=c]
	02 -> 01 [label=a]
	02 -> 0 [label=b]
	02 -> 2 [label=c]
}
